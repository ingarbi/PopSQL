
SELECT name, major 
FROM student;

SELECT student.name, student.major 
FROM student
ORDER BY name;

SELECT student.name, student.major 
FROM student
ORDER BY name DESC;

SELECT * 
FROM student
ORDER BY student_id DESC;

SELECT * 
FROM student
ORDER BY student_id ASC;

SELECT * 
FROM student
ORDER BY major, student_id DESC;

SELECT * 
FROM student
ORDER BY student_id DESC
LIMIT 2;


SELECT * 
FROM student
WHERE major = 'Biology';

SELECT name, major
FROM student
WHERE major = 'Sociology';

SELECT name, major
FROM student
WHERE major = 'Sociology' or major = 'Chemistry';

SELECT name, major
FROM student
WHERE major = 'Sociology' or name = 'Claire';

SELECT name, major
FROM student
WHERE major <> 'Chemistry'; --not equal

SELECT *
FROM student
WHERE student_id < 3 ; 

SELECT *
FROM student
WHERE name IN ('Claire', 'Kate', 'Mike')
